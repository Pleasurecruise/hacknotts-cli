{
  "name": "hacknotts-cli",
  "version": "1.0.0",
  "description": "Interactive AI-powered CLI for HackNotts 2025 - Chat with multiple AI providers through an elegant terminal interface",
  "engines": {
    "node": ">=22.12.0"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsdown && node docs/scripts/post-build.js && npm install -g . --force",
    "dev": "tsc -w",
    "clean": "rm -rf dist packages/*/dist",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --cache",
    "lint:check": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cli",
    "ai",
    "chatbot",
    "terminal",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "gemini",
    "hackathon",
    "hacknotts",
    "ai-chat",
    "interactive-cli",
    "ai-providers",
    "mcp",
    "ink",
    "react"
  ],
  "author": "Pleasurecruise, IvanHanloth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pleasurecruise/hacknotts-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Pleasurecruise/hacknotts-cli/issues"
  },
  "homepage": "https://github.com/Pleasurecruise/hacknotts-cli#readme",
  "bin": {
    "hacknotts": "dist/cli.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./aiCore": {
      "types": "./dist/aiCore/index.d.ts",
      "default": "./dist/aiCore/index.js"
    },
    "./aiCore/built-in/plugins": {
      "types": "./dist/aiCore/built-in/plugins/index.d.ts",
      "default": "./dist/aiCore/built-in/plugins/index.js"
    },
    "./aiCore/provider": {
      "types": "./dist/aiCore/provider/index.d.ts",
      "default": "./dist/aiCore/provider/index.js"
    },
    "./toolkit": {
      "types": "./dist/toolkit/index.d.ts",
      "default": "./dist/toolkit/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.27",
    "@ai-sdk/azure": "^2.0.49",
    "@ai-sdk/deepseek": "^1.0.23",
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/openai": "^2.0.48",
    "@ai-sdk/openai-compatible": "^1.0.22",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.12",
    "@ai-sdk/xai": "^2.0.26",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "ai": "^5.0.26",
    "dotenv": "^17.2.3",
    "ink": "^6.3.1",
    "react": "^19.2.0",
    "zod": "^4.1.5",
    "ink-gradient": "^3.0.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.36.1",
    "@types/node": "^22.10.1",
    "@types/react": "^19.2.2",
    "@eslint/js": "^9.22.0",
    "eslint": "^9.22.0",
    "eslint-plugin-import-zod": "^1.2.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.4.0",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^4.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": [
      "eslint --fix"
    ]
  }
}
